{
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "ms-python.autopep8",
  "autopep8.args": [
    "--select=E,W",
    "--max-line-length",
    "119",
    "--experimental"
  ],
  "mypy-type-checker.args": [
    "--strict",
    "--disable-error-code no-untyped-call",
    "--disable-error-code misc"
  ],
}